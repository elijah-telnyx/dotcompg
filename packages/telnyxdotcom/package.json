  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "docker:build": "docker build -t telnyxdotcom -f Dockerfile $(cat .env.local | sed '/^$/d' | sed 's@^@--build-arg @g') ../../.",
    "docker:start": "docker run --name telnyxdotcom -d -p 3000:3000 $(cat .env.local | sed '/^$/d' | sed 's@^@--env @g') telnyxdotcom",
    "format": "npx prettier --write .",
    "git:log": "git log --pretty='%C(yellow)%ad%C(reset) %C(blue)[%an]%C(reset) %s' --date=short '.' '../ui'",
    "lint": "eslint . --fix",
    "local:https": "npx local-ssl-proxy --key ../../certificates/localhost-key.pem --cert ../../certificates/localhost.pem --source 3001 --target 3000",
    "setup-env:local": "./setup-env.sh",
    "start": "next start",
    "test": "jest"
  },
